{
    "name": "da-eto-ya/behat-spawner-extension",
    "description": "Spawns processes when Behat starts",
    "type": "library",
    "keywords": ["behat", "spawner", "process", "extension"],
    "homepage": "https://github.com/da-eto-ya/behat-spawner-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pleshakov",
            "email": "da-eto@ya.ru"
        }
    ],

    "minimum-stability": "stable",

    "require": {
        "php": ">=5.3.4",
        "behat/behat": "~2.5",
        "symfony/process": ">=2.3.12"
    },

    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },

    "autoload": {
        "psr-0": {
            "DaEtoYa\\Behat\\SpawnerExtension": "src/"
        }
    },

    "config": {
        "bin-dir": "bin"
    }
}